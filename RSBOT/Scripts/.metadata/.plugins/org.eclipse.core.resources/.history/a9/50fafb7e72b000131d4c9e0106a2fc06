import org.powerbot.script.methods.Bank;
import org.powerbot.script.methods.MethodContext;
import org.powerbot.script.wrappers.Area;
import org.powerbot.script.wrappers.Item;


public class Withdraw extends Task{
	
	public boolean large,medium,small;
	public boolean flag;
	public int largePouch = 5513;
	public int mediumPouch = 5510;
	public int smallPouch = 5509;
	public int pureEssence = 7936;
	
	
	Area arg;
	public Withdraw(MethodContext ctx) {
		super(ctx);
	}

	@Override
	public boolean activate() {
	
		
		return ctx.backpack.select().count() < 28;
						
	}

	@Override
	public void excecute() {
				withdraw();
				fill();
				withdraw();
				
	}
	public void withdraw(){
		ctx.bank.open();
		ctx.bank.withdraw(smallPouch, 1);
		ctx.bank.withdraw(mediumPouch, 1);
		ctx.bank.withdraw(largePouch, 1);
		ctx.bank.withdraw(pureEssence, Bank.Amount.ALL);
		}
	public void fill(){
		
		Item SmallPouch = ctx.backpack.getItemAt(smallPouch); 
		
		Item MediumPouch = ctx.backpack.getItemAt(mediumPouch);
		
		Item LargPouch = ctx.backpack.getItemAt(largePouch);
			ctx.bank.close();
			SmallPouch.interact("Fill Small Pouch");
			System.out.println("Filling the pouch");
			MediumPouch.interact("Fill Medium Pouch");
						
			LargPouch.interact("Fill Larg Pouch");
			
		
	
	}
	}


